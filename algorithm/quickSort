function partition(A,p,r){
    let x  = A[r];
    let i = p-1;
    for(let j = p; j<= r-1; j++){
        if(A[j]<= x){
            i = i + 1;
            let temp = A[j];
            A[j] = A[i]; 
            A[i] = temp;
        }
    }
    let temp = x;
    A[r] = A[i+1];
    A[i+1] = temp;
    return i+1;
}

function quickSort(A, p,r){
    if(p<r){
        let q = partition(A,p,r);
        quickSort(A,p,q-1);
        quickSort(A,q+1, r);
    }
}